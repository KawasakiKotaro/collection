<%= form_for(@review)  do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %> <br>
  <%= f.label :text %>
  <%= f.text_area :text %><br>
  <%= f.file_field :image %><br>
  <%= f.label :'評価' %>
  <%= f.number_field :rate, placeholder:'評価', value: 1, max: 10, min: 1 %><br>
  <%= f.submit "送信する" %>
  
    <% end %>
    
    