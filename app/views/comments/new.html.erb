<%= form_for(@comment) do |f| %>
    <%= f.label :nickname %>
    <%= f.text_field :nickname %>
    <%= f.label :comment %>
    <%= f.text_field :comment %><br>
    <%= f.label :rate %>
    <%= f.number_field :rate, placeholder:'評価' , value: 1, max: 10, min: 1 %>
    <%= f.submit "送信する" %>
<% end %>
    